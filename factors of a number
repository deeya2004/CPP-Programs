#include<iostream>
using namespace std;
int main() {
//program to find factors of a number
   int num , i;
   cout << "Enter a number:" ;
   cin>>num;
   
   cout << "The factors of " << num << " are : ";
   for(i=1; i <= num; i++) {
      if (num % i == 0)
      cout << i << " ";
   }
   return 0;
};
