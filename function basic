#include<iostream>
using namespace std;
//function
int power(){
  int a,b;
  cout<<"enter the value of a:"<<endl;
  cin>>a;
  cout<<"enter the value of b:"<<endl;
  cin>>b;
  int ans=1;
  for (int i=1;i<=b;i++){
    ans=ans*a;
  }
  return ans;
}
int main(){
  int ans=power();
  cout<<"answer is "<<ans<<endl;
  return 0;
};
