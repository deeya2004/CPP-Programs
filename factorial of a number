#include <iostream>
using namespace std;

int main() {
  //code to print the factorial of a number.
  int n;
  cout << "Enter the number:";
  cin >> n;
  int factorial = 1;
  for (int i = 1; i <= n; i++) {
    factorial *= i;
  }
  cout << "The factorial of " << n << " is: " << factorial << endl;
  return 0;
};
