#include <iostream>
using namespace std;
int main() {
  // switch case
  int num;
  cout << "Enter the value of num:" << endl;
  cin >> num;
  char ch;
  cout << "Enter the value of ch:" << endl;
  cin >> ch;

  cout << endl;
  switch (num) {
  case 1:
    cout << "first" << endl;
    break;
  case 2:
    switch (ch) {
    case '1':
      cout << "Value of ch is:" << ch << endl;
      break;
    case '2':
      cout << "Value of ch is:" << ch << endl;
      break;
    case '3':
      cout << "Value of ch is:" << ch << endl;
      break;
    case '4':
      cout << "Value of ch is:" << ch << endl;
      break;
    default:
      cout << "default character: " << ch << endl;
    }
    break;
  case 3:
    cout << "third" << endl;
    break;
  case 4:
    cout << "fourth" << endl;
    break;
  default:
    cout << "nothing";
  }
  return 0;
};
